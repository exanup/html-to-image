{
  "compilerOptions": {
    "declaration": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "module": "es6",
    "target": "es6",
    "outDir": "./lib",
    "lib": ["es6", "dom", "es2015"]
  },
  "include": ["src/**/*.ts"]
}
